{
  "name": "sensarion-api-v2",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "openapi:validate": "swagger-cli validate openapi/openapi.yaml",
    "openapi:diff": "echo 'OpenAPI diff not yet implemented'",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:dry": "prisma migrate dev --create-only",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "dep:scan": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "^5.18.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "swagger-cli": "^4.0.4",
    "tsx": "^4.15.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}

